{
	"name": "DataflowSilverGold_SalesOrderHeader",
	"properties": {
		"folder": {
			"name": "Child flows/Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "SalesOrderHeaderSilver"
				},
				{
					"linkedService": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "CustomerSilver"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "GetCusomer"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     container: 'syn-fs',",
				"     folderPath: 'delta/silver/SalesOrderHeader') ~> SalesOrderHeaderSilver",
				"source(output(",
				"          CompanyName as string,",
				"          CustomerID as string,",
				"          EmailAddress as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          MiddleName as string,",
				"          ModifiedDate as string,",
				"          NameStyle as string,",
				"          PasswordHash as string,",
				"          PasswordSalt as string,",
				"          Phone as string,",
				"          SalesPerson as string,",
				"          Suffix as string,",
				"          Title as string,",
				"          rowguid as string,",
				"          SourceFileName as string,",
				"          DateInserted as timestamp,",
				"          CompanyName_Prev as string,",
				"          UserName as string,",
				"          Domain as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     container: 'syn-fs',",
				"     folderPath: 'delta/silver/Customer') ~> CustomerSilver",
				"SalesOrderHeaderSilver, CustomerSilver lookup(ERROR_FUNCTION('') == CustomerID,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> GetCusomer"
			]
		}
	}
}