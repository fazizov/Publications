{
	"name": "Deduplication_Flowlet",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Window1"
				},
				{
					"name": "Filter1"
				},
				{
					"name": "Select1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          IDColumn as long,",
				"          OrderColum as timestamp",
				"     ),",
				"     order: 0) ~> input1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: '',",
				"     tableName: '') ~> source1",
				"input1 window(over(PrimaryKey = IDColumn),",
				"     asc(OrderColum, true),",
				"     RowNum = rowNumber()) ~> Window1",
				"Window1 filter(RowNum==1) ~> Filter1",
				"Filter1 select(mapColumn(",
				"          IDColumn,",
				"          OrderColum",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Select1",
				"Select1 output() ~> output1"
			]
		}
	}
}